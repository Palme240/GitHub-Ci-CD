{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "name": "githubcosmosdb",
      "type": "cosmosDB",
      "direction": "in",
      "ConnectionStringSetting": "CosmosDBConnection",
      "databaseName": "GitHubFunction",
      "collectionName": "DBitem",
      "partitionKey": "/all"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return",
      "dataType": "string" 
    }
  ],
  "disabled": false
}
